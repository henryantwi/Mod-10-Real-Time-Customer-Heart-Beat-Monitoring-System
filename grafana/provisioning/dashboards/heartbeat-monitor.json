                {
                    "datasource": {
                        "type": "postgres",
                        "uid": "heartbeat-postgres"
                    },
                    "editorMode": "code",
                    "format": "table",
                    "rawQuery": true,
                    "rawSql": "SELECT\n  customer_id,\n  COUNT(*)                              AS total_readings,\n  ROUND(AVG(heart_rate)::numeric, 1)    AS avg_hr,\n  MIN(heart_rate)                       AS min_hr,\n  MAX(heart_rate)                       AS max_hr,\n  SUM(CASE WHEN is_anomaly THEN 1 ELSE 0 END) AS anomaly_count\nFROM heartbeat_readings\nWHERE $__timeFilter(recorded_at)\nGROUP BY customer_id\nORDER BY customer_id;",
                    "refId": "A"
                }
            ],
            "title": "Customer Statistics",
            "type": "table"
        },
